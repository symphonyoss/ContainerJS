  "license": "Apache-2.0",
  "dependencies": {
    "mocha": "^3.2.0",
    "openfin-cli": "^1.1.5",
    "containerjs-api-specification": "0.0.1",
    "containerjs-api-openfin": "0.0.1",
    "containerjs-api-tests": "0.0.1",
    "openfin-launcher": "^1.3.12"
  },
  "devDependencies": {
    "copyfiles": "^1.2.0",
    "live-server": "^1.2.0",
    "npm-run-all": "^4.0.2",
    "rimraf": "^2.6.1",
    "spectron": "3.2.3"
  }
